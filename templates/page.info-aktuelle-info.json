        },
        "71709b28-2f50-4146-b31c-ff29852b75b1": {
          "type": "person",
          "settings": {
            "ava": "shopify:\/\/shop_images\/Vector_1.png",
            "name": "12 Aus- und Weiterbildungen",
            "text": "Wir legen grossen Wert auf ständige Weiterentwicklung und Bildung, um stets auf dem neuesten Stand in Bezug auf Kinderwunsch, Schwangerschaft, Geburt, Baby und Familie zu sein."
          }
        },
        "e3a0d4fe-d2e6-4e55-881b-903312ced00c": {
