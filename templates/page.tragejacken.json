    "44c01f17-18ce-4124-bc46-25f6a8583cc9": {
      "type": "rich-text",
      "blocks": {
        "1bfb9937-fb74-4a42-90b6-2a09d3ac6ba5": {
          "type": "text",
          "settings": {
            "text": "<h2>Accessoires<\/h2><p>Wenn es kalt ist, sind die richtigen Accessoires für dein Kind Gold wert. Auch hier setzen wir hauptsächlich auf Produkte aus Wolle oder Wolle-Seide.<\/p>"
          }
        }
      },
      "block_order": [
